{
  "manifest_version": 2,

  "web_accessible_resources": [
    "rice_logo_from_xml.png",
    "women_swimming.png",
    "women_basketball.png",
    "women_tennis.png",
    "women_volleyball.png",
    "men_baseball.png",
    "men_basketball.png",
    "men_track.png",
    "women_soccer.png",
